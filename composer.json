{
  "name": "matt127127/laravel-migration-paths",
  "description": "Multiple migration paths for Laravel projects.",
  "type": "library",
  "keywords": [
    "laravel",
    "migrations"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Matthew Treagus",
      "email": "matt@treagus.me"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0",
    "illuminate/console": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "matt127127\\MigrationPath\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "matt127127\\MigrationPath\\ServiceProvider",
        "matt127127\\MigrationPath\\MigrationServiceProvider"
      ]
    }
  }
}
